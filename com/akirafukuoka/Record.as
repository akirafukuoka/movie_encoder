package com.akirafukuoka {	import com.akirafukuoka.Block;		public class Record {				private var _frame:int = 0;		private var _timeCode:Number = 0;		private var _block:Block;		private var _array:Array = [];		public function Record(frame:int,timeCode:Number,w_num:int,h_num:int) {			// constructor code			_frame = frame;			_timeCode = timeCode;			for(var i:int=0;i<w_num;i++){				_array[i] = [];				for(var k:int=0;k<h_num;k++){					_array[i][k] = [];				}			}		}		public function get frame():int {			return _frame;		}		public function get timeCode():Number {			return _timeCode;		}		public function setBlockNum(w_num:int,h_num:int,num:int):void {			_array[w_num][h_num] = num;		}		public function getBlockNum(w_num:int,h_num:int):int {			return _array[w_num][h_num];		}	}	}